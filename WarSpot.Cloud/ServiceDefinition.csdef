<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="WarSpot.Cloud" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2012-05.1.7">
  <WorkerRole name="WarSpot.Cloud.MatchComputer" vmsize="Small">
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <LocalResources>
    </LocalResources>
    <ConfigurationSettings>
      <Setting name="DataConnectionString" />
    </ConfigurationSettings>
  </WorkerRole>
  <WorkerRole name="WarSpot.Cloud.UserService" vmsize="Small">
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <Endpoints>
      <InputEndpoint name="ServiceEndpoint" protocol="tcp" port="10100" />
    </Endpoints>
    <ConfigurationSettings>
      <Setting name="DataConnectionString" />
    </ConfigurationSettings>
  </WorkerRole>
</ServiceDefinition>